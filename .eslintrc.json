{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb",
        "airbnb-typescript"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "project": "tsconfig.json"
    },
    "plugins": [
        "react"
    ],
    "rules": {
      "no-plusplus": "off",
      "import/no-cycle": "off",
      "@typescript-eslint/lines-between-class-members": "off",
      "import/prefer-default-export": "off",
      "react/no-array-index-key": "off",
      "no-new": "off",
      "click-events-have-key-events": "off"
    }
}
